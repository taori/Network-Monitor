<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter="clr-namespace:NetworkMonitor.Framework.Mvvm.Converter;assembly=NetworkMonitor.Framework.Mvvm"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

	<converter:BooleanToVisibilityConverter TrueVisibility="Visible" FalseVisibility="Collapsed" x:Key="BooleanToVisibilityConverter"></converter:BooleanToVisibilityConverter>
	<converter:BooleanToVisibilityConverter TrueVisibility="Collapsed" FalseVisibility="Visible" x:Key="BooleanToVisibilityInversedConverter"></converter:BooleanToVisibilityConverter>
	<converter:BooleanInversionConverter  x:Key="BooleanInversionConverter"></converter:BooleanInversionConverter>
	<system:String x:Key="DateFormat">dd.MM.yyyy hh:mm:ss</system:String>
</ResourceDictionary>